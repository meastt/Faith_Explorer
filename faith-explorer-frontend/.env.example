# Faith Explorer Frontend Configuration
# Copy this file to .env and add your actual values

# RevenueCat Public API Key (safe for frontend)
# Get your PUBLIC key from: https://app.revenuecat.com/ → Project Settings → API Keys
# IMPORTANT: Use the PUBLIC key (starts with "rcb_"), NOT the secret key!
VITE_REVENUECAT_PUBLIC_KEY=your_revenuecat_public_key_here

# API Base URL (optional, defaults to http://localhost:3001)
VITE_API_URL=http://localhost:3001
